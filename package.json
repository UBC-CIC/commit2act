  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test:watch": "react-scripts test --watchAll=true",
    "test:basic": "react-scripts test --watchAll=false",
    "test:coverage": "react-scripts test --watchAll=false --coverage",
    "test": "npm run test:watch",
    "lint:fix": "eslint . --fix",
    "lint": "eslint .",
    "eject": "react-scripts eject",
    "checkup": "npm run lint:fix && npm run test:once"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/*.js",
      "src/App.js",
      "!src/actions/*",
      "!src/constants/*",
      "!src/graphql/*",
      "!src/localization/*",
      "!src/models/*",
      "!src/reducers/*",
      "!src/styles/*"
    ],
    "clearMocks": true,
    "resetMocks": true
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "axe-core": "^4.8.3",
    "eslint-plugin-react": "^7.33.2",
    "jest-axe": "^8.0.0",
    "jest-environment-jsdom": "^29.7.0"
